// FlatBuffers IDL file

namespace example_versioning_protocol;

table Ack
{
  status : int;
}

table PutRequest
{
  key   : string;
  value : string;
}

table GetRequest
{
  key           : string;
  version_index : int;
}

table GetResponse
{
  modified_timestamp : uint64;
  value_len          : uint64;
}

union Element
{
  PutRequest,
  GetRequest,
  GetResponse,
  Ack,
}

table Message
{
  transaction_id : uint64;
  element : Element;
}

root_type Message;
